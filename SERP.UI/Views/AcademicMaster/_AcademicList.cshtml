@model IEnumerable<SERP.Core.Entities.Entity.Core.Master.AcademicMaster>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-12">
                    <table id="tblAcademicList" class="table table-striped table-bordered responsive" style="width:100%">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Start Date</th>
                                <th>End Date</th>
                                <th>Description</th>
                                <th>Action</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var data in Model)
                            {
                                <tr>
                                    <td>
                                        @data.Name
                                    </td>
                                    <td>
                                        @data.StartDate.ToShortDateString()
                                    </td>
                                    <td>
                                        @data.EndDate.ToShortDateString()
                                    </td>
                                    <td>
                                        @data.Description
                                    </td>
                                    <td>
                                        <a class="btn btn-primary pull-right" onclick="GetAcademicList()">
                                            <i class="fa fa-edit"></i>
                                            Edit
                                        </a>
                                        <a class="btn btn-danger pull-right" onclick="GetAcademicList()">
                                            <i class="fa fa-trash"></i>
                                            Delete
                                        </a>
                                    </td>
                                </tr>
                            }
                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>
</section>

